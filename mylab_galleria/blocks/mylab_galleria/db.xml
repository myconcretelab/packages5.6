		<field name="carousel"	 	type="L"></field>
		<field name="carouselFollow" 	type="L"></field>
		<field name="carouselSpeed" 	type="I"></field>
		<field name="carouselSteps" 	type="C" size="15"></field>
		<field name="history" 		type="L"></field>
		<field name="imageCrop" 	type="L"></field>
		<field name="imageCropPhp" 	type="L"></field>
